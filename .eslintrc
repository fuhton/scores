{
	"extends": "airbnb",
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"class-methods-use-this": 0,
		"indent": [2, "tab", {"SwitchCase": 1}],
		"jsx-a11y/no-static-element-interactions": 0,
		"no-console": 0,
		"no-tabs": 0,
		"react/forbid-prop-types": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"react/jsx-indent": 0,
		"strict": 0
	},
	"plugins": [
		"react"
	],
}
